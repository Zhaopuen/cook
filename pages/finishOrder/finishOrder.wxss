/* pages/finishOrder/finishOrder.wxss */
navigator {
  width:100% !important;
}
.show{
  display:block;
}
.hidden{
  display:none;
}
.receipt_index{
  width:100%;
  height:100%;
  overflow:hidden;
  background:#35bdb6;
  padding-bottom: 120rpx;
  overflow-y:auto;
  position:fixed;
}
page{
  width: 100%;
  height: 100%;
}
.container{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.finishtime{
  color:#aaa !important;
}
.nav {
  height: 120rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  /* line-height: 80rpx; */
  color:#fff;
  font-size: 16px;
  white-space: nowrap;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  padding:0 20rpx;
  background:#35bdb6;
}
.nav-item {
  width: 25%;
  display: inline-block;
  text-align: center;
}
.wait_finish{
  width:100%;
  height:auto;
  overflow: hidden;
}
.wait_finishbox{
  width:86%;
  height:auto;
  overflow: hidden;
  background:#fff;
  border-radius:40rpx;
  padding:4%;
  margin:30rpx auto;
}
.wait_finishbox image{
  width:180rpx;
  height:180rpx;
  display: block;
  float:left;
  margin-right: 20rpx;
  border-radius:40rpx;
}
.wait_finishtitle{
  height:auto;
  overflow: hidden;
  float:left;
}
.wait_finishtitle text{
  color:#666;
  display: block;
  font-size: 32rpx;
  margin-bottom: 30rpx;
}
.wait_finishtitle span{
  color:#666;
  display: block;
  font-size: 32rpx;
}
.wait_finished{
  height:auto;
  overflow: hidden;
  float:right;
}
.wait_finished text{
  display: block;
  color:#35bdb6;
  margin-bottom: 50rpx;
  font-size: 34rpx;
  text-align: right;
}
.finished{
  color:#aaa !important;
}
.wait_finished span{
  display: block;
  color:#35bdb6;
  font-size: 32rpx;
  padding-top: 40rpx;
}
.all_list_finished view{
  float:right;
  width:250rpx;
  background-color: #35bdb6;
	border-radius: 35px;
  line-height: 80rpx;
  color:#fff;
  font-size: 32rpx;
  text-align: center;
}
.nav_itembox{
  display: block;
  width:90%;
  height:auto;
  overflow: hidden;
  padding:3%;
  margin: 0 auto;
  /* position: fixed;
  top:0; */
  background: #35bdb6;
  text-align: center;
}
.nav_itembox .nav-item{
  color:#fff;
  opacity: 0.5;
  font-size: 32rpx;
  margin:20rpx 0;
}
.nav-item p{
  display: block;
}
.nav_itembox .active{
  color:#fff;
  opacity: 1;
}




.finish_order{
  width:100%;
  height:auto;
  overflow: hidden;
  background:#35bdb6;
}
.finish_date{
  width:90%;
  height:auto;
  overflow: hidden;
  padding:5%;
  margin:0 auto;
  color:#fff;
  font-size: 30rpx;
}
.finish_date text{
  display: block;
  float:left;
  color:#fff;
  font-size: 32rpx;
}
.finish_date image{
  width:40rpx;
  height:40rpx;
  float:right;
  display: block;
}
/* 列表 */
.all_list{
  width:86%;
  height:auto;
  overflow: hidden;
  background:#fff;
  margin:0 auto 30rpx;
  border-radius:40rpx;
  padding:3%;
}
.all_list_header{
  /* width:90%; */
  height:auto;
  overflow: hidden;
  margin:0 auto;
  padding:20rpx 0rpx;
  border-bottom:1px solid #f0f0f0;
}
.all_list_header .all_list_num{
  width:48rpx;
  height:48rpx;
  border-radius:50%;
  border:5rpx solid #AFE5E2;
  text-align: center;
  line-height: 48rpx;
  font-size: 24rpx;
  color:#35bdb6;
  float:left;
  margin-right: 20rpx;
}
.all_list_header .all_list_name{
  color:#aaaaaa;
  font-size: 30rpx;
  line-height: 52rpx;
  float:left;
}
.all_list_header .all_list_time{
  color:#35bdb6;
  font-size: 35rpx;
  float:right;
}
.all_list_content{
  /* width:95%; */
  height:auto;
  overflow: hidden;
  margin:0 auto;
}
.all_list_content .all_list_listbox{
  width:44%;
  height:auto;
  padding:3%;
  overflow: hidden;
  float:left;
  margin:0 0rpx 60rpx;
}
.all_list_content .all_list_listbox .all_list_fanname{
  font-size: 30rpx;
  line-height: 50rpx;
  margin-bottom: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.all_list_content .all_list_listbox .all_list_number{
  font-size: 35rpx;
  float:left;
  line-height: 60rpx;
}
.all_list_content .all_list_listbox .all_list_addnum{
  width: 110rpx;
	height: 60rpx;
	background-color: #ff6204;
	border-radius: 20px;
	opacity: 0.7;
  float:left;
  margin-left: 40rpx;
  text-align: center;
  line-height: 60rpx;
  color:#fff;
}
.all_list_finished{
  width:100%;
  height:auto;
  overflow: hidden;
}
.all_list_finished span{
  float:left;
  font-size:30rpx;
  color:#aaaaaa;
  line-height: 70rpx;
}
.all_list_finished p{
  float:right;
  line-height: 70rpx;
  font-size: 30rpx; 
  color:#35bdb6;
}

/* 遮罩层 */
.maskBox{
  width:100%;
  height:calc(100% - 100rpx);
  position: fixed;
  top:340rpx;
  z-index:99999;
  margin:auto;
  left:0; 
  right:0;
  bottom:0;
  background:rgba(0,0,0,0.4);
}
.maskMiddle{
  width:100%;
  height:auto;
  overflow-y:auto;
  background:#fff;
  position: absolute;
  top: 0rpx;
}
.mask_text{
  width:100%;
  height:auto;
  overflow: hidden;
}
.mask_text text{
  display: block;
  /* color:#666; */
  font-size: 34rpx;
  padding:5% 6%;
  height:auto;
  overflow: hidden;
  border-bottom: 1px solid #f0f0f0;
}
.select_date{
  font-size: 30rpx;
  color:#fff;
  width:90%;
  height:auto;
  overflow: hidden;
  padding:0% 5% 5%;
}
.select_date text{
  display: block;
  float:left;
}
.select_date image{
  width:50rpx;
  height:50rpx;
  display: block;
  float:right;
}
.dabao{
  width:135rpx;
  height:60rpx;
  color:#fff;
  background:#ff6204;
  border-radius:50rpx;
  text-align:center;
  line-height:60rpx;
  float:left;
  font-size:28rpx;
  margin-left:20rpx;
}